in:
  type: jdbc
  driver_path: "C:/drivers/ojdbc8.jar"
  driver_class: 'oracle.jdbc.driver.OracleDriver'
  url: jdbc:oracle:thin:@localhost:1521:orcl
  database: DEMO
  user: MYUSER
  password: ABCD
  table: customers
  column_options:
    id: {value_type: long}
filters:
  - type: oracle_lookup
    url: jdbc:oracle:thin:@localhost:1521:orcl
    driver_path: "C:/drivers/ojdbc8.jar"
    driver_class: 'oracle.jdbc.driver.OracleDriver'
    table_name: car
    username: MYUSER
    password: ABCD
    mapping_from:
      - car_name
      - company
    mapping_to:
      - name_of_car
      - brand
    new_columns:
      - { name: car_id, type: string }
      - { name: category, type: string }
      - { name: fuel_capacity, type: string }

out:
  type: jdbc
  driver_path: "C:/drivers/ojdbc8.jar"
  driver_class: 'oracle.jdbc.driver.OracleDriver'
  url: jdbc:oracle:thin:@localhost:1521:orcl
  database: DEMO
  user: MYUSER
  password: ABCD
  table: output
  mode: insert_direct
